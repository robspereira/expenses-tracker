<ejs-sidebar id="sidebar" width="290px" enableDock dockSize="100px" target=".dock-target">
    <e-content-template>
        
        <div class="logo-wrapper">
            <div class="app-logo">
                <img src="~/logo.png"/>
            </div>
            <div class="w-100"></div>
            <i id="sidebar-toggler" class="fa-solid"></i>
        </div>
        
        <div class="profile-wrapper">
            <img class="profile-pic" src="~/profile.png"/>
            <div class="titles d-flex flex-column ps-3">
                <h6 class="mb-0">Controle de Finanças</h6>
                <span class="text-muted">Roberto Lucas</span>
            </div>
        </div>

        <ejs-menu id="menu" items="menuItems" orientation="Vertical">

        </ejs-menu>
    </e-content-template>
</ejs-sidebar>

<script>
    document.addEventListener('DOMContentLoaded', function(){
        dockBar = document.getElementById('sidebar').ej2_instances[0];
        document.getElementById('sidebar-toggler').onclick = function(){
            dockBar.toggle();
        };
        
    })
    

</script>